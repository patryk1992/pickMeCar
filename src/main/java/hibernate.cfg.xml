<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
		"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
	<session-factory>
		<property name="dialect">org.hibernate.dialect.MySQLDialect</property>
		<property name="show_sql">true</property>
		<mapping resource="com/mgr/pickMeCar/db/model/Token.hbm.xml" />
		<mapping resource="com/mgr/pickMeCar/db/model/Groups.hbm.xml" />
		<mapping resource="com/mgr/pickMeCar/db/model/UserGroup.hbm.xml" />
		<mapping resource="com/mgr/pickMeCar/db/model/User.hbm.xml" />
		<mapping resource="com/mgr/pickMeCar/db/model/Mark.hbm.xml" />
		<mapping resource="com/mgr/pickMeCar/db/model/UserTrack.hbm.xml" />
		<mapping resource="com/mgr/pickMeCar/db/model/UserCar.hbm.xml" />
		
		<mapping resource="com/mgr/pickMeCar/db/model/Through.hbm.xml" />
		<mapping resource="com/mgr/pickMeCar/db/model/Track.hbm.xml" />
		<mapping resource="com/mgr/pickMeCar/db/model/Car.hbm.xml" />
		<mapping resource="com/mgr/pickMeCar/db/model/City.hbm.xml" />
	</session-factory>
</hibernate-configuration>
